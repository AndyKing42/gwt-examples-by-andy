<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:c='urn:import:com.google.gwt.user.cellview.client'
  xmlns:cce='urn:import:org.greatlogic.runtimecssexample.client'>
  <g:DockLayoutPanel unit='EM'>
    <g:north size='2'>
      <g:HorizontalPanel>
        <g:Label>Phone Number</g:Label>
        <cce:CustomCellWidget
          ui:field='customCellWidget'
          readOnly='false'
          separator='-'
          value=''
          width='100px'/>
      </g:HorizontalPanel>
    </g:north>
    <g:west size='20'>
      <c:DataGrid
        ui:field='dataGrid1'
        width='100%'
        height='100%'/>
    </g:west>
    <g:west size='20'>
      <c:DataGrid
        ui:field='dataGrid2'
        width='100%'
        height='100%'/>
    </g:west>
  </g:DockLayoutPanel>
</ui:UiBinder>