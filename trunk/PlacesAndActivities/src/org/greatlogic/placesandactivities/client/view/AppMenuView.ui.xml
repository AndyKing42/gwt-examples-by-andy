<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'>
  <g:ResizeLayoutPanel ui:field='menuPanel'>
    <g:MenuBar
      animationEnabled='true'
      autoOpen='false'
      ui:field='menuBar'>
      <g:MenuItem>
        People
        <g:MenuBar vertical='true'>
          <g:MenuItem>Foster Maintenance</g:MenuItem>
          <g:MenuItem>Foster Type Maintenance</g:MenuItem>
        </g:MenuBar>
      </g:MenuItem>
      <g:MenuItem>
        Pets
        <g:MenuBar vertical='true'>
          <g:MenuItem ui:field='petMaintenanceMenuItem'>Pet Maintenance</g:MenuItem>
          <g:MenuItem>Pet Type Maintenance</g:MenuItem>
        </g:MenuBar>
      </g:MenuItem>
      <g:MenuItem>
        System
        <g:MenuBar vertical='true'>
          <g:MenuItem ui:field='orgMaintenanceMenuItem'>Organization Maintenance</g:MenuItem>
          <g:MenuItem>System Configuration</g:MenuItem>
          <g:MenuItem>User Maintenance</g:MenuItem>
        </g:MenuBar>
      </g:MenuItem>
    </g:MenuBar>
  </g:ResizeLayoutPanel>
</ui:UiBinder>