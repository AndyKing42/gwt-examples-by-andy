<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
  <g:DockLayoutPanel unit='EM'>
    <g:north size="10">
      <g:Button ui:field="button">Push</g:Button>
    </g:north>
    <g:north size="10">
      <g:TextBox ui:field="textBox"></g:TextBox>
    </g:north>
    <g:west size='20'>
      <c:DataGrid
        ui:field='dataGrid'
        width='100%'
        height='100%'/>
    </g:west>
    <g:west size='20'>
      <c:DataGrid
        ui:field='dataGrid2'
        width='100%'
        height='100%'/>
    </g:west>
  </g:DockLayoutPanel>
</ui:UiBinder>